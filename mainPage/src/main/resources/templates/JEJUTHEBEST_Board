CREATE TABLE QABOARD(
qaBoardNo INT AUTO_INCREMENT,
qaBoardTitle VARCHAR(255) NOT NULL,
qaBoardContents TEXT,
qaBoardAnswer boolean DEFAULT FALSE,
qaBoardDate DATETIME DEFAULT CURRENT_TIMESTAMP(),
category_id VARCHAR(45) NOT NULL,
user_id VARCHAR(45) NOT NULL,
views INT NOT NULL DEFAULT 0,
CONSTRAINT PRIMARY KEY(qaBoardNo),
FOREIGN KEY(category_id) REFERENCES CATEGORY(category_id)
ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY(user_id) REFERENCES USER(user_id)
ON UPDATE CASCADE ON DELETE CASCADE
);

insert into qaboard
(qaBoardTitle, qaBoardContents, qaBoardAnswer, category_id, user_id) VALUES 
('test', 'test', '0', '1', 'admin'),
('test', 'test', '0', '2', 'admin');

CREATE TABLE QAREPLY(
qaReplyNo INT AUTO_INCREMENT,
qaReplyContent TEXT NOT NULL,
qaReplyDate DATETIME DEFAULT CURRENT_TIMESTAMP(),
qaBoardNo INT,
CONSTRAINT PRIMARY KEY(qaReplyNo),
FOREIGN KEY(qaBoardNo) REFERENCES QABOARD(qaBoardNo)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE TOURIMG(
tour_img_no INT AUTO_INCREMENT,
tour_rank INT NOT NULL,
img_path VARCHAR(255) NOT NULL,
PRIMARY KEY(tour_img_no),
FOREIGN KEY(tour_rank) REFERENCES TOUR(tour_rank)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE RESTAURANTIMG(
restaurank_img_no INT AUTO_INCREMENT,
rest_rank INT NOT NULL,
img_path VARCHAR(255) NOT NULL,
PRIMARY KEY(restaurank_img_no),
FOREIGN KEY(rest_rank) REFERENCES RESTAURANT(rest_rank)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE ACCOIMG(
acco_img_no INT AUTO_INCREMENT,
acco_rank INT NOT NULL,
img_path VARCHAR(255) NOT NULL,
PRIMARY KEY(acco_img_no),
FOREIGN KEY(acco_rank) REFERENCES ACCOMMODATE(acco_rank)
ON UPDATE CASCADE ON DELETE CASCADE
);

