CREATE TABLE NOTICE_IMG(
notice_img_no INT AUTO_INCREMENT,
notice_no INT NOT NULL,
img_path VARCHAR(255) NOT NULL,
PRIMARY KEY(notice_img_no),
FOREIGN KEY(notice_no) REFERENCES NOTICE(notice_no)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE CATEGORY(
category_id VARCHAR(45) NOT NULL,
category_name VARCHAR(45) NOT NULL,
PRIMARY KEY(category_id)
);

INSERT INTO CATEGORY(category_id, category_name) VALUES
('1','관광지'),
('2','숙박'),
('3','맛집');

CREATE TABLE ACCOMMODATE(
acco_rank INT AUTO_INCREMENT,
tourist VARCHAR(255) NOT NULL,
province VARCHAR(255) NOT NULL,
city VARCHAR(255) NOT NULL,
address1 VARCHAR(255) NOT NULL,
search INT(45) NOT NULL,
views INT  NOT NULL DEFAULT 0,
acco_phone VARCHAR(20),
contents TEXT,
user_id VARCHAR(45) NOT NULL,
category_id VARCHAR(255),
clean FLOAT DEFAULT 0,
bedding FLOAT DEFAULT 0,
room FLOAT DEFAULT 0,
price FLOAT DEFAULT 0,
location FLOAT DEFAULT 0,
service FLOAT DEFAULT 0,
PRIMARY KEY(acco_rank),
FOREIGN KEY(user_id) REFERENCES USER(user_id)
ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY(category_id) REFERENCES CATEGORY(category_id)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE RESTAURANT(
rest_rank INT AUTO_INCREMENT,
tourist VARCHAR(255) NOT NULL,
province VARCHAR(255) NOT NULL,
city VARCHAR(255) NOT NULL,
address2 VARCHAR(255) NOT NULL,
search INT(45) NOT NULL,
views INT  NOT NULL DEFAULT 0 ,
rest_phone VARCHAR(20),
contents TEXT,
user_id VARCHAR(45) NOT NULL,
category_id VARCHAR(255),
price FLOAT DEFAULT 0,
taste FLOAT DEFAULT 0,
location FLOAT DEFAULT 0,
service FLOAT DEFAULT 0,
mood FLOAT DEFAULT 0,
PRIMARY KEY(rest_rank),
FOREIGN KEY(user_id) REFERENCES USER(user_id)
ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY(category_id) REFERENCES CATEGORY(category_id)
ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE TOUR(
tour_rank INT AUTO_INCREMENT,
tourist VARCHAR(255) NOT NULL,
province VARCHAR(255) NOT NULL,
city VARCHAR(255) NOT NULL,
address3 VARCHAR(255) NOT NULL,
category_id VARCHAR(255),
search INT(45) NOT NULL,
views INT  NOT NULL DEFAULT 0 ,
tour_phone VARCHAR(20),
contents TEXT,
user_id VARCHAR(45) NOT NULL,
PRIMARY KEY(tour_rank),
FOREIGN KEY(user_id) REFERENCES USER(user_id)
ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY(category_id) REFERENCES CATEGORY(category_id)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE BOARD(
board_no INT AUTO_INCREMENT,
place_name  VARCHAR(45) NOT NULL,
rating FLOAT NOT NULL,
title VARCHAR(255) NOT NULL,
post_time DATETIME DEFAULT CURRENT_TIMESTAMP(),
contents  TEXT NOT NULL,
address1 VARCHAR(255),
address2 VARCHAR(255),
address3 VARCHAR(255),
views INT NOT NULL DEFAULT 0,
good INT NOT NULL DEFAULT 0,
bad INT  NOT NULL DEFAULT 0,
user_id VARCHAR(45) NOT NULL,
category_id VARCHAR(45) NOT NULL,
PRIMARY KEY(board_no),
FOREIGN KEY(user_id) REFERENCES USER(user_id)
ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY(category_id) REFERENCES CATEGORY(category_id)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE BOARD_IMG(
board_img_no INT AUTO_INCREMENT,
board_no INT NOT NULL,
img_path VARCHAR(255) NOT NULL,
PRIMARY KEY(board_img_no),
FOREIGN KEY(board_no) REFERENCES BOARD(board_no)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE REPLY(
reply_no INT AUTO_INCREMENT,
contents VARCHAR(255) NOT NULL,
post_time DATETIME DEFAULT CURRENT_TIMESTAMP(),
img_path VARCHAR(255) DEFAULT NULL,
board_no INT NOT NULL,
user_id VARCHAR(255) NOT NULL,
good INT NOT NULL DEFAULT 0,
bad INT  NOT NULL DEFAULT 0,
PRIMARY KEY(reply_no),
FOREIGN KEY(user_id) REFERENCES USER(user_id)
ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY(board_no) REFERENCES BOARD(board_no)
ON UPDATE CASCADE ON DELETE CASCADE
);

CREATE TABLE BOARD_PREFER(
board_prefer_no INT AUTO_INCREMENT,
board_no INT NOT NULL,
prefer BOOLEAN,
user_id VARCHAR(255) NOT NULL,
PRIMARY KEY (board_prefer_no),
FOREIGN KEY (user_id) REFERENCES USER(user_id)
ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY (board_no) REFERENCES BOARD(board_no)
ON UPDATE CASCADE ON DELETE CASCADE,
UNIQUE KEY (board_no, user_id)
);

CREATE TABLE REPLY_PREFER(
reply_prefer_no INT AUTO_INCREMENT,
reply_no INT NOT NULL,
prefer BOOLEAN,
user_id VARCHAR(255) NOT NULL,
PRIMARY KEY (reply_prefer_no),
FOREIGN KEY (user_id) REFERENCES USER(user_id)
ON UPDATE CASCADE ON DELETE CASCADE,
FOREIGN KEY (reply_no) REFERENCES REPLY(reply_no)
ON UPDATE CASCADE ON DELETE CASCADE,
UNIQUE KEY (reply_no,user_id)
);

